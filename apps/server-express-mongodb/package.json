{
  "name": "server-express-mongodb",
  "version": "1.0.0",
  "description": "A RealWorld Backend API made with express + prisma",
  "main": "dist/server.js",
  "scripts": {
    "build": "dotenv -e .env -- npx tsc",
    "start": "node dist/server.js",
    "start:debug": "DEBUG=express:* node dist/server.js",
    "dev": "set DEBUG=express:* & dotenv -e .env -- npx ts-node-dev --rs --clear --exit-child src/api/index.ts",
    "lint": "eslint --fix src/**/*.ts",
    "style": "npx prettier --check .",
    "test": "dotenv -e .env -- jest -i",
    "test:watch": "dotenv -e .env -- jest -i --watch --coverage=false"
  },
  "keywords": [
    "express",
    "RealWorld",
    "API",
    "Backend"
  ],
  "author": "Ronan Pardo Soares",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "dotenv-cli": "^6.0.0",
    "jest": "^29.2.1",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.3.0",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.8.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.8.1",
    "mongoose-unique-validator": "^3.1.0",
    "slugify": "^1.6.5",
    "validator": "*"
  }
}
